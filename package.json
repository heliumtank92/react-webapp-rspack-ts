{
  "name": "react-webapp-rspack-ts",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "rsbuild build",
    "browsers": "npx browserslist",
    "browsers:lint": "npx browserslist-lint",
    "biome:lint": "biome check",
    "check": "biome check --write",
    "dev": "rsbuild dev --open",
    "doctor": "RSDOCTOR=true npm run build",
    "format": "biome format --write",
    "lint": "npm run tsc:lint && npm run biome:lint",
    "preview": "rsbuild preview --open",
    "supportedBrowsers": "echo \"module.exports = $(browserslist-useragent-regexp --allowHigherVersions);\" > ./public/static/js/supportedBrowsers.js",
    "tsc:lint": "tsc"
  },
  "dependencies": {
    "@am92/react-design-system": "^2.7.7",
    "@am92/web-http": "^2.0.28",
    "@fontsource-variable/inter": "^5.1.1",
    "@reduxjs/toolkit": "^2.5.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.1.1",
    "react-router-dom": "^7.1.1",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "utility-types": "^3.11.0"
  },
  "devDependencies": {
    "@aaroon/workbox-rspack-plugin": "^0.3.2",
    "@biomejs/biome": "^1.9.4",
    "@rsbuild/core": "^1.1.13",
    "@rsbuild/plugin-assets-retry": "^1.0.7",
    "@rsbuild/plugin-basic-ssl": "^1.1.1",
    "@rsbuild/plugin-image-compress": "^1.1.0",
    "@rsbuild/plugin-node-polyfill": "^1.2.0",
    "@rsbuild/plugin-react": "^1.1.0",
    "@rsbuild/plugin-sass": "^1.1.2",
    "@rsdoctor/rspack-plugin": "^0.4.13",
    "@rspack/core": "^1.1.8",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "browserslist-useragent-regexp": "^4.1.3",
    "code-inspector-plugin": "^0.20.0",
    "favicons": "^7.2.0",
    "htmlparser2": "^10.0.0",
    "image-minimizer-webpack-plugin": "^4.1.3",
    "lefthook": "^1.10.1",
    "rsbuild-plugin-ejs": "^1.0.1",
    "rsbuild-plugin-html-minifier-terser": "^1.1.1",
    "typescript": "^5.7.3"
  }
}
